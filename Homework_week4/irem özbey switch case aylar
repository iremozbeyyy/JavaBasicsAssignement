import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner scanner= new Scanner(System.in);

        System.out.println("ay girin");
        String ay=scanner.nextLine();
        switch (ay) {
            case "aralık" :
            case "ocak":
            case "şubat":
                System.out.println("mevsim kış");
                break;
            case "mart" :
            case "nisan":
            case "mayis":
                System.out.println("mevsim ilkbahar");
                break;
            case "haziran":
            case "temmuz":
            case "ağustos":
                System.out.println("mevsim yaz");
            case "eylül":
            case "ekim":
            case "kasım":
                System.out.println("mevsim sonbahar");
                break; 
                default:
                System.out.println("yanlış girdiniz");
                break;
        }

    } }
